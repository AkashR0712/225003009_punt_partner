{
    "version": 2,
    "name": "language_translate",
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/media/(.*)", "dest": "/media/$1" },
      { "src": "/(.*)", "dest": "/app/$1" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "language_translate.settings",
      "DATABASE_URL": "",
      "SECRET_KEY": ""
    }